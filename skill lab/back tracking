#include<stdio.h>
#include<stdbool.h>
#define N 4
int board [N][N];
void printboard ()
{
    for(int i=0;i,N;i++){
        for (int j=0;j<N;j++)
        printf("%c",board[i][j]?'Q':'-');
    }
    printf("\n");
}
pr
ntf("\n");
}
bool issafe(int ro,int col)
{
    for (inti=0;i<col;i++)
    if(board[row][i])
    return false;
    for(inti=0;j=col;i>=0&&j>=0;i--,j--)
    if (board[i][j])
    return false;
    for (int i=row,j=col;i<N&&j>=0;i+=,j==0
    if (board[i][j])
    return false;
    return true;
}
bool saolveNqueens(int col)
{
    if(col==N){
        printboard();
        return true;
    }
    bool res=false;
    for(int i=0;i<N;i++)
    {
        iif(issafe(i,col))
        {
            board[i][col]=1;
            res=solveNqueens(col+1)||res;
            board[i][col]=0;
        }
    }
    return res;
}
int main()
{
    for(int i=0;i<N;i++)
    for(int j=0;j<N;j++)
    board[i][j]=0;
    solveNqueens(0);
    return 0;
}
